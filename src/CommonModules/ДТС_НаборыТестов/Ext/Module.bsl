Функция НаборТестовСправочника(ОписаниеМетаданных) Экспорт 
    
    НаборТестов = Новый Массив;
    НаборТестов.Добавить(ДТС_Тесты.ОписаниеТестаОткрытьФормуСписка(ОписаниеМетаданных));
    
    // TODO
    Если НЕ ОписаниеМетаданных.Подчиненный Тогда
        НаборТестов.Добавить(ДТС_Тесты.ОписаниеТестаОткрытьФормуВыбора(ОписаниеМетаданных));
        НаборТестов.Добавить(ДТС_Тесты.ОписаниеТестаОткрытьФормуОбъекта(ОписаниеМетаданных));
        НаборТестов.Добавить(ДТС_Тесты.ОписаниеТестаОткрытьФормуСуществующегоОбъекта(ОписаниеМетаданных));
        НаборТестов.Добавить(ДТС_Тесты.ОписаниеТестаЗаписатьСуществующийОбъект(ОписаниеМетаданных));
        
        Если ОписаниеМетаданных.Иерархический Тогда
            НаборТестов.Добавить(ДТС_Тесты.ОписаниеТестаОткрытьФормуВыбораГруппы(ОписаниеМетаданных));
            НаборТестов.Добавить(ДТС_Тесты.ОписаниеТестаОткрытьФормуГруппы(ОписаниеМетаданных));
            НаборТестов.Добавить(ДТС_Тесты.ОписаниеТестаОткрытьФормуСуществующейГруппы(ОписаниеМетаданных));
            НаборТестов.Добавить(ДТС_Тесты.ОписаниеТестаЗаписатьСуществующуюГруппу(ОписаниеМетаданных));
        КонецЕсли;
    КонецЕсли;
    
    ДополнитьНаборТестовМакетамиСКД(НаборТестов, ОписаниеМетаданных);
    Возврат НаборТестов;
    
КонецФункции

