// 
////
Процедура ДобавитьНаборТестов(Идентификатор, Представление, НаборТестов) Экспорт 
    
    ЮТТесты.ДобавитьТестовыйНабор(Идентификатор);
    
    Для Каждого Тест Из НаборТестов Цикл
        Если ДТС_Настройки.Пропустить(Тест.Идентификатор) Тогда
            Возврат;
        КонецЕсли;
        
        ЮТТесты.ДобавитьТест(Тест.Метод, Тест.Представление).СПараметрами(Тест.Параметры);
    КонецЦикла;
    
КонецПроцедуры

#КонецОбласти

