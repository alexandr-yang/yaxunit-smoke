// 
////
Функция НаборТестовСправочники(Иерархический, Подчиненный, Теги) Экспорт 
    
    Массив = Новый Массив;
    Массив.Добавить(ДымовыеТесты_Тесты.ОписаниеТестаОткрытьФормуСписка(Теги));
    
    Если Подчиненный Тогда
        // TODO
        Возврат Массив;
    КонецЕсли;
    
    Массив.Добавить(ДымовыеТесты_Тесты.ОписаниеТестаОткрытьФормуВыбора(Теги));
    Массив.Добавить(ДымовыеТесты_Тесты.ОписаниеТестаОткрытьФормуОбъекта(Теги));
    
    Если Иерархический Тогда
        Массив.Добавить(ДымовыеТесты_Тесты.ОписаниеТестаОткрытьФормуВыбораГруппы(Теги));
        Массив.Добавить(ДымовыеТесты_Тесты.ОписаниеТестаОткрытьФормуГруппы(Теги));
    КонецЕсли;
    
    Возврат Массив;
    
КонецФункции

